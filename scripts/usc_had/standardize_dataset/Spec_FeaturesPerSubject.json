                "constraints": {
                    "required": "TRUE",
                    "minimum": "10",
                    "maximum": "50"
                }
            },
            {
                "name": "height",
                "type": "numeric",
                "role": "measurement",
                "description": "Height of the subject",
                "units": "centimeters",
                "constraints": {
                    "required": "TRUE",
                    "minimum": "0",
                    "maximum": "200"
                }
            },
            {
                "name": "weight",
                "type": "numeric",
                "role": "measurement",
                "description": "Weight of the subject",
                "units": "kilograms",
                "constraints": {
                    "required": "TRUE",
                    "minimum": "0",
                    "maximum": "200"
