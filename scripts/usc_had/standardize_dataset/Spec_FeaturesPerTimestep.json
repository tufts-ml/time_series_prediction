{
    "name": "FeaturesPerTimestep",
    "path": "features_per_timestep.csv",
    "format": "csv",
    "mediatype": "text/csv",
    "profile": "tabular-data-resource",
    "$schema": "https://frictionlessdata.io/schemas/table-schema.json",
    "schema": {
        "fields": [
            {
                "name": "subject_id",
                "type": "integer",
                "role": "key",
                "description": "Unique identifier per subject of the experiment",
                "units": "NONE",
                "constraints": {
                    "required": "TRUE",
                    "minimum": "1",
                    "maximum": "14"
                }
            },
            {
                "name": "trial",
                "type": "integer",
                "role": "key",
                "description": "Indicator for trial performed per activity per subject.",
                "units": "NONE",
                "constraints": {
                    "required": "TRUE",
                    "minimum": "1",
                    "maximum": "5"
                }
            },
            {
                "name": "seq_id",
                "type": "integer",
                "role": "key",
                "description": "In data files organized by subject>trial&activity",
                "units": "NONE",
                "constraints": {
                    "required": "TRUE",
                    "minimum": "1",
                    "maximum": "12"
                }
            },
            {
                "name": "tstep",
                "type": "integer",
                "role": "timestep",
                "description": "sensor data (1000s x 6) Varying # of rows per file",
                "units": "NONE",
                "constraints": {
                    "required": "TRUE",
                    "minimum": "0",
                    "maximum": "INF"
                }
            },
            {
                "name": "acc_x",
                "type": "numeric",
                "role": "measurement",
                "description": "accelerometer x sensor data",
                "units": "g (gravity)",
                "constraints": {
                    "required": "TRUE",
                    "minimum": "-INF",
                    "maximum": "INF"
                }
            },
            {
                "name": "acc_y",
                "type": "numeric",
                "role": "measurement",
                "description": "accelerometer y sensor data",
                "units": "g (gravity)",
                "constraints": {
                    "required": "TRUE",
                    "minimum": "-INF",
                    "maximum": "INF"
                }
            },
            {
                "name": "acc_z",
                "type": "numeric",
                "role": "measurement",
                "description": "accelerometer z sensor data",
                "units": "g (gravity)",
                "constraints": {
                    "required": "TRUE",
                    "minimum": "-INF",
                    "maximum": "INF"
                }
            },
            {
                "name": "gyro_x",
                "type": "numeric",
                "role": "measurement",
                "description": "gyroscpe x sensor data",
