            {
                "name": "gyro_y",
                "type": "numeric",
                "role": "measurement",
                "description": "gyroscpe y sensor data",
                "units": "dps (degrees per second)",
                "constraints": {
                    "required": "TRUE",
                    "minimum": "-INF",
                    "maximum": "INF"
                }
            },
            {
                "name": "gyro_z",
                "type": "numeric",
                "role": "measurement",
                "description": "gyroscpe z sensor data",
                "units": "dps (degrees per second)",
                "constraints": {
                    "required": "TRUE",
                    "minimum": "-INF",
                    "maximum": "INF"
                }
            }
        ],
        "missingValues": [
            "",
            "nan"
        ]
    }
